isbn = {0165-7836},
issn = {01657836},
journal = {Fisheries Research},
keywords = {Area- and fleet-based management,Fishery-dependent data (logbooks),Satellite data,Skagerrak-Kattegat,Spatial and temporal fishing effort distribution,Vessel monitoring system (VMS)},
month = {oct},
