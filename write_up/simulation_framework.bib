keywords = {VMS data,cod,conservation credits,discards,real-time closures},
month = {jul},
number = {8},
pages = {1679--1688},
title = {{Using fishery-dependent data to inform the development and operation of a co-management initiative to reduce cod mortality and cut discards}},
url = {http://icesjms.oxfordjournals.org/cgi/doi/10.1093/icesjms/fsr101},
