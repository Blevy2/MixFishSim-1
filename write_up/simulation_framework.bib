@article{Tidd2012,
abstract = {A discrete choice model is applied to determine how fishing effort is allocated spatially and temporally by the English and Welsh North Sea beam trawl fleet. Individual vessels can fish in five distinct areas, and the utility of fishing in an area depends on expected revenue measured as previous success (value per unit effort) and experience (past fishing effort allocation), as well as perceived costs (measured as distance to landing port weighted by fuel price). The model predicts fisher location choice, and the predictions are evaluated using iterative partial cross validation by fitting the model over a series of separate time-periods (nine separate time-periods). Results show the relative importance of the different drivers that change over time. They indicate that there are three main drivers throughout the study, past annual effort, past monthly effort in the year of fishing, and fuel price, largely reflecting the fact that previous practices where success was gained are learned (i.e. experience) and become habitual, and that seasonal variations also dominate behaviour in terms of the strong monthly trends and variable costs. In order to provide an indication of the model's predictive capabilities, a simulated closure of one of the study areas was undertaken (an area that mapped reasonably well with the North Sea cod 2001 partial closure of the North Sea for 10 weeks of that year). The predicted reallocation of effort was compared against realized/observed reallocation of effort, and there was good correlation at the trip level, with a maximum 10{\%} misallocation of predicted effort for that year. ?? 2012.},
author = {Tidd, Alex N. and Hutton, Trevor and Kell, Laurence T. and Blanchard, Julia L.},
doi = {10.1016/j.fishres.2012.03.004},
file = {:home/paul/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Tidd et al. - 2012 - Dynamic prediction of effort reallocation in mixed fisheries.pdf:pdf},
isbn = {0165-7836},
issn = {01657836},
journal = {Fisheries Research},
keywords = {Closed areas,Decision-making,Effort reallocation,Flatfish,Implementation error,Location choice,Random utility model,Spatial management,Value per unit effort},
month = {aug},
pages = {243--253},
publisher = {Elsevier B.V.},
title = {{Dynamic prediction of effort reallocation in mixed fisheries}},
url = {http://dx.doi.org/10.1016/j.fishres.2012.03.004},
volume = {125-126},
year = {2012}
